-card.jsx":"import React from 'react';\nimport PropTypes from 'prop-types';\nimport Button from 'react-bootstrap/Button';\nimport Card from 'react-bootstrap/Card';\nimport Container from 'react-bootstrap/Container';\n\nimport './movie-card.scss';\n\nexport class MovieCard extends React.Component {\n  render() {\n    const { movie, onClick } = this.props;\n\n    return (\n      <Container className=\"card-container\">\n        <Card className=\"movie-card\">\n          <Card.Img className=\"movie-poster\" variant=\"top\" src={movie.ImagePath} />\n          <Card.Body>\n            <Card.Title className=\"movie-title\">{movie.Title}</Card.Title>\n            <Card.Text className=\"movie-description\">{movie.Description}</Card.Text>\n            <Button onClick={() => onClick(movie)} variant=\"primary\" size=\"lg\" block className=\"open-button\">Open</Button>\n          </Card.Body>\n        </Card>\n      </Container>\n    );\n  }\n}\n\nMovieCard.propTypes = {\n  movie: PropTypes.shape({\n    ImagePath: PropTypes.string.isRequired,\n    Title: PropTypes.string.isRequired,\n    Description: PropTypes.string.isRequired\n  }).isRequired,\n  onClick: PropTypes.func.isRequired\n};"},"lineCount":null}},"error":null,"hash":"f7ed57d79b4af3aa18c70c483678156f","cacheData":{"env":{}}}